class celebrities{
  public static void main(String args[]){
  String s1 = "Allan Alda";
  String s2 = "John Wanye";
  String s3 = "Gregory Peck";
  
  int allan = s1.length();
  System.out.println(allan);
  int john = s2.length();
  System.out.println(john);
  int greg = s3.length();
  System.out.println(greg);
  
  
  }
}
